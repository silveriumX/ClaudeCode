{
  "mcpServers": {
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-exa"
      ],
      "env": {
        "EXA_API_KEY": "${env:EXA_API_KEY}"
      }
    },
    "firecrawl": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-firecrawl"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "${env:FIRECRAWL_API_KEY}"
      }
    }
  }
}
