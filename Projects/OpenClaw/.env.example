# VPS SSH Access
VPS_HOST=your-vps-ip
VPS_PORT=22
VPS_USER=root
VPS_PASSWORD=your-ssh-password

# Alternative: use VOICEBOT_SSH_PASS for backward compatibility
# VOICEBOT_SSH_PASS=your-ssh-password
# VOICEBOT_HOST=your-vps-ip
# VOICEBOT_USER=root

# Anthropic API Key
ANTHROPIC_API_KEY=sk-ant-api...
# Alternative variable name used in some scripts
# OPENCLAW_ANTHROPIC_KEY=sk-ant-api...

# Z.AI API Key (optional, for GLM-4.7 model)
ZAI_API_KEY=sk-zai-api...
# Alternative variable name
# OPENCLAW_ZAI_API_KEY=sk-zai-api...

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=1234567890:ABC-your-bot-token
# Alternative variable name
# OPENCLAW_TELEGRAM_BOT_TOKEN=1234567890:ABC-your-bot-token
